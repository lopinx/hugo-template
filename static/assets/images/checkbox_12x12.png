			type: data.form.method,
			data: $(data.form).serialize()+"&provinceName="+provinceName+"&cityName="+cityName+"&sourceTxt=JDWY站点",
			success: function (info) {
				if (info.code === 1) {
					layer.msg(info.msg);
					setTimeout(function () {
						window.location.reload()
					}, 1000);
				} else {
					layer.msg(info.msg);
					layer.closeAll('loading');
				}
			}
		});
		return false;
	});

